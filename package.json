{
	"name": "healnew",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"dev:host": "vite dev --host",
		"build": "vite build",
		"build:analyze": "vite build --mode analyze",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"optimize": "npm run build && npm run preview",
		"perf": "npm run build && lighthouse http://localhost:4173 --output=html --output-path=./lighthouse-report.html"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@internationalized/date": "^3.8.1",
		"@lucide/svelte": "^0.513.0",
		"@melt-ui/svelte": "^0.76.0",
		"@sveltejs/adapter-vercel": "^5.5.2",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/postcss": "^4.1.8",
		"@tailwindcss/vite": "^4.0.0",
		"@tanstack/table-core": "^8.21.3",
		"@types/d3-scale": "^4.0.9",
		"@types/d3-shape": "^3.1.0",
		"@types/nprogress": "^0.2.3",
		"autoprefixer": "^10.4.20",
		"bits-ui": "^2.5.0",
		"clsx": "^2.1.1",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.14.0",
		"layerchart": "2.0.0-next.10",
		"nprogress": "^0.2.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-toolbelt": "^0.9.1",
		"tailwind-merge": "^3.0.2",
		"tailwind-variants": "^1.0.0",
		"tailwindcss": "^4.0.0",
		"terser": "^5.43.1",
		"tw-animate-css": "^1.3.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.0.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	},
	"dependencies": {
		"d3-scale": "^4.0.2",
		"d3-shape": "^3.2.0"
	}
}
